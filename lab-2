/1/
#include <stdio.h>
int main()
{
    float d,r;
    scanf("%f", &r);
    d=r*3.14;
    printf("%f", d);
    return 0;
}

/2/
#include <stdio.h>
#include <math.h>
int main()
{
    float a,b,c,d,x1,x2;
    scanf("%f%f%f",&a,&b,&c);
    d=b*b-4*a*c;
    if (d>=0)
    {
        x1=(-b+sqrt(d))/(2*a);
        x2=(-b-sqrt(d))/(2*a);
        printf("x1=%f, x2=%f",x1,x2);
    }
    return 0;
}

/3/
#include <stdio.h>
int main()
{
    printf("Input n\n");
    int n,fac;
    scanf("%d",&n);
    fac=1;
    for (int i=1; i<=n; i++)
    {
        fac*=i;
    }
    printf("Factorial=%d", fac);
}
